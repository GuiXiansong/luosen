/**
 * 店铺角色（总店/分店）
 */
public enum StoreRoles {
    HEADOFFICE("总店",1),
    BRANCH("分店",2);

    private String name;
    private int index;

    StoreRoles(String name, int index) {
        this.name = name;
        this.index = index;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getIndex() {
        return index;
    }

    public void setIndex(int index) {
        this.index = index;
    }
}
